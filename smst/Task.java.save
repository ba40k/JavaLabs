public class Task{
    public static String subtask1(int a, int b){
        return (a + "+" + b + "=" + (a+b));
    }
    public static int subtask2(String a){
        int ans =0;
        int add = 1;
        for (int i = 0;i<a.length();i++){
            char c = a.charAt(i);
            if (c == ' ') {
                ans += add;
                add = 1;
                continue;
            }
            if (Character.toLowerCase(c) >'z' || Character.toLowerCase(c)<'a') add = 0;
        }
        ans += add;
        return ans;
    }
    public static StringBuilder subtask3(String a){
        StringBuilder res = new StringBuilder();
        for (int i =0;i<a.length();i++){
            char c = a.charAt(i);
            if (c>='0' && c <='9') continue;
            res.append(c);
        }
        return res;
    }
